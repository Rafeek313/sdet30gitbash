package EclipseDemo;

public class B extends A {
void display() {
	super.display();
	System.out.println("its B");
}
void display(int a) {
	a=10;
	super.a=20;
	int c=a+super.a;
	System.out.println(c);
	System.out.println("it is argument B");
}
public static void main(String[] args) {
	B b=new B();
	b.display(10);
}
}
